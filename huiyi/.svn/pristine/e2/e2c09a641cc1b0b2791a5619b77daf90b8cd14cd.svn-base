@charset "UTF-8";
/* reset */
@font-face {
  font-family: 'FontAwesome';
  src: url('../wxFont/fontawesome-webfont.eot?v=3.2.1');
  src: url('../wxFont/fontawesome-webfont.eot?#iefix&v=3.2.1') format('embedded-opentype'), url('../wxFont/fontawesome-webfont.woff?v=3.2.1') format('woff'), url('../wxFont/fontawesome-webfont.ttf?v=3.2.1') format('truetype'), url('../wxFont/fontawesome-webfont.svg#fontawesomeregular?v=3.2.1') format('svg');
  font-weight: normal;
  font-style: normal;
}
.FontAwesome:before{font-family: 'FontAwesome';margin-right:5px;}
html{width:100%;height:100%;-ms-text-size-adjust:100%;-webkit-text-size-adjust:100%;}body{width:100%;height:100%;line-height: 1.6;font-size:62.5%;font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;color:#000;position:relative;}body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,button,textarea,p,blockquote,th,td{margin:0;padding:0;}table{border-collapse:collapse;border-spacing:0;font-size:inherit;}img{border:0 none}li{list-style:none}h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal}input,button,textarea,select,optgroup,option{font-family:inherit;font-size:inherit;font-style:inherit;font-weight:inherit}a{text-decoration:none;outline:none;color:inherit}a:hover{text-decoration:none}.clear{overflow:hidden;clear:both;float:none}input,button,select,textarea{outline:none}textarea{resize:none}
::-webkit-input-placeholder{color:#c6c6c6;}
.full{width:100%;overflow:hidden;}.left{float:left;display:inline-block;}.right{float:right;display:inline-block;}
input[type="text"], input[type="date"], input[type="search"], input[type="tel"]{border:0 none;font-size:1rem;padding:0 5px;color:#000;height:24px;white-space:nowrap;}
input[type="date"]{-webkit-appearance:none;background:none}
input[type="date"].emptyDate:before{ content: attr(placeholder);color:#c6c6c6;}
.select:after{content:'';display:inline-block;width:20px;height:100%;background:url(images/selectIcon.png) no-repeat center center;background-size:100%;float:right;margin-right:0.5rem}
select{border:0 none;-webkit-appearance:none;background:#fff;height:24px;font-size:1rem;padding-right:30px;}
textarea{border:1px solid #c6c6c6;}
a.fullButton{width:100%;height:40px;line-height:40px;text-align:center;display:block;border-radius:5px;font-size:1rem}
a.greenButton{background:#83cf51;color:#fff;}



.box{background:#fff;padding:10px; padding-top:45px; box-sizing:border-box; overflow:scroll;}
.noPaddingTop{padding:0 10px;}
.user{border:1px solid #e3e3e3;border-radius:5px;display:block;overflow:hidden;position:relative;}
.user .car{width:60px;display:inline-block;position:absolute;left:5px;}
.user .car img{display:block;width:100%;}
.user .userName{font-size:1rem;color:#353535;border-right:1px dashed #e3e3e3;margin:0 65px 0 60px;}
.user .userName span{font-size:0.8rem;display:block;color:#c4c4c4}
.user .userFace{position:absolute;right:10px;top:7px;overflow:hidden;width:52px;height:52px;}
.user .userFace img{border:1px solid #e3e3e3;border-radius:50%;padding:1px;width:48px;height:48px;}
.user .userFace .redDot{background:#ff2122;width:10px;height:10px;border-radius:50%;border:2px solid #fff;position:absolute;right:0;top:0;}

.functionLists{overflow:hidden;width:105%}
.functionLists a{width:45%;padding:15px 0;margin:10px 10px 0 0;display:inline-block;float:left;text-align:center;border:1px solid #e3e3e3;color:#474747;border-radius:5px;}

.formList{border:1px solid #e3e3e3;border-radius:5px;margin-bottom:10px;font-size:1rem;}
.formList li{height:2rem;line-height:2rem;border-bottom:1px solid #e3e3e3;padding:0.4rem 0;}
.formList li:last-child{border-bottom:0 none}
.formList li label{display:inline-block;width:6.5rem;text-align:right;border-right:1px dashed #e3e3e3;padding-right:0.5rem;margin-right:0.5rem}
.formList li select, .formList li input[type="text"], .formList li input[type="date"], .formList li input[type="tel"]{width:48%;}
.formList li.textarea{height:auto;padding:0.4rem 10px;}
.formList li textarea{padding:5px 0;width:100%;height:5rem;border:0 none;font-size:1rem;}

.pageList{border:1px solid #e3e3e3;border-radius:5px;}
.pageList li{border-bottom:1px solid #e3e3e3;padding:0.4rem 0;overflow:hidden;height:80px;font-size:1rem;color:#474747;}
.pageList li:last-child{border-bottom:0 none;}
.pageList li a{width:100%;height:100%;overflow:hidden;position:relative;display:block;}
.pageList li img{float:left;display:inline-block;width:110px;height:80px;margin-right:1rem}
.pageList li .time{position:absolute;bottom:0;left:8rem;font-size:0.8rem}
.pageList li .view{position:absolute;bottom:0;right:0;font-size:0.8rem}
.pageList li .view:before{content:'\f06e';color:#bebebe;}

/*投诉列表*/
.pageList li .tousu_title{width:8rem; display:block; }
.pageList li .guwen_name{width:5rem; display:block; position:absolute; top:0rem; right:3.5rem; }
.pageList li .zhuangtai{width:3.5rem; background: #F90; display:block; text-align:center; border-radius:12px; position:absolute; top:0.1rem; right:0rem; font-size:0.8rem; color:#FFF; }
.pageList li .time1{position:absolute;bottom:0;left:0rem;font-size:0.85rem; color:#999;}
.pageList li .concent{ width:95%;position:absolute;bottom:33%;left:0rem;font-size:0.9rem; color:#888; overflow:hidden; white-space: nowrap;text-overflow:ellipsis;}

/*试驾列表*/
.pageList li .drivelist{ width:76%;position:absolute; top:0rem;left:0rem;}
.pageList li .btn_back{ width:2.8rem; height:1.4rem; background:#fff; border:1px solid #CCC; border-radius:0.2rem;position:absolute; bottom:0rem;right:0.4rem; font-size:0.8rem; color:#999;}


.linkList{border:1px solid #e3e3e3;border-radius:5px;margin-bottom:10px;overflow:hidden;}
.linkList li{height:44px;line-height:44px;border-bottom:1px solid #e3e3e3;font-size:1rem}
.linkList li:last-child{border-bottom:0 none;}
.linkList li a{width:100%;height:100%;overflow:hidden;display:block;text-indent:1rem;color:#474747;position:relative;}
.linkList li a:after{font-family:'FontAwesome';content:'\f105';color:#bbbbbb;float:right;height:44px;line-height:44px;margin-right:0.8rem;font-size:1rem;}
.linkList li .till{color:#95ca35;font-size:0.8rem;position:absolute;right:2rem}
.linkList li.userPhoto{height:45px;line-height:45px;padding:0.4rem 0;font-size:1.2rem}
.linkList li.userPhoto img{width:45px;height:45px;vertical-align:middle;border-radius:5px;margin-right:0.5rem}
.linkList li.toggle a:after{content:none;}
.linkList li.toggle .toogleButton{position:absolute;right:1rem;top:50%;margin-top:-14px;border:1px solid #e3e3e3;border-radius:14px;height:28px;line-height:28px;overflow:hidden;font-size:0.8rem;text-indent:0}
.linkList li.toggle .toogleButton i{background:#cbcbcb;color:#fff;font-style:normal;width:40px;display:inline-block;text-align:center;}
.linkList li.toggle .toogleButton b{background:#fff;color:#707070;position:absolute;top:0;left:-1px;width:45px;border-radius:14px;text-align:center;-webkit-transition:left 0.3s linear}
.linkList li.toggle .toogleButton b.right{left:35px}

#pageInfo .title{font-size:2.2rem;font-weight:bold;line-height:24px;margin-bottom:10px;}
#pageInfo .pageMain img{max-width:100%;}
#pageInfo .list li{font-size:1rem;font-weight:bold;}

.zhantingTitle{border-bottom:1px solid #e3e3e3;position:relative;padding:5px 15px;font-size:0.9rem;color:#474747;height:30px;line-height:30px;}
.zhantingTitle .logo{width:20px;vertical-align:middle;}
.zhantingTitle span{position:absolute;right:15px;font-size:0.8rem;color:#bbb;height:30px;line-height:30px;top:5px;}
.zhantingList{width:100%;overflow:hidden;}
.zhantingList li{width:50%;float:left;display:inline-block;text-align:center;font-size:0.8rem;margin:0 0 0.8rem;white-space:nowrap;}
.zhantingList li a{display:block;overflow:hidden;width:100%;}
.zhantingList li img{display:block;margin:5px auto;}
.zhantingList li span{display:block;width:100%;color:#bbb;font-size:0.6rem}
.zhantingList:last-child{margin-bottom:55px;}

.zhantingListInLine li{width:100%;border-bottom:1px solid #e3e3e3;height:50px;line-height:50px;overflow:hidden;text-align:left;margin:0;padding:10px 0}
.zhantingListInLine li a{height:100%;text-indent:15px}
.zhantingListInLine li img{display:inline-block;vertical-align:middle;margin-right:0.8rem;border-radius:5px;}
.zhantingListInLine li a:after{font-family:'FontAwesome';content:'\f105';color:#bbbbbb;float:right;height:50px;line-height:50px;margin-right:0.8rem;font-size:1rem;}

#footerLinks{background:#eeeeee;width:100%;overflow:hidden;height:54px;position:fixed;bottom:0;left:0;border-top:1px solid #e3e3e3;z-index:9}
#footerLinks a{width:24%;float:left;display:inline-block;border-right:1px solid #e3e3e3;font-size:0.7rem;color:#515151;text-align:center;line-height:54px;height:54px;}
#footerLinks a:last-child{border-right:0 none;}

.carTitle{overflow:hidden;position:relative;padding:15px;}
.carTitle .pic{width:90px;height:50px;position:absolute;display:block;top:15px;left:15px;overflow:hidden;}
.carTitle .pic img{width:100%;height:100%;display:block;}
.carTitle .name, .carTitle span{display:block;margin-left:110px;}
.carTitle .name{font-size:0.9rem;color:#3d3d3d;}
.carTitle .name:after{font-family:'FontAwesome';content:'\f105';width:15px;height:15px;display:inline-block;vertical-align:text-top;margin-left:5px;text-align:center;line-height:15px;background:#84cd5b;color:#fff;border-radius:50%;}
.carTitle span{color:#d2d2d2;font-size:0.8rem;}
.carTitle span label{color:#f5b76e}
.carTitle span.thisPrice{color:#fca43a}
.carList{font-size:0.8rem;line-height:18px;}
.carList li{position:relative;border-bottom:1px solid #e3e3e3;padding:15px}
.carList li a{width:70px;height:28px;display:block;border-radius:5px;color:#fff;position:absolute;line-height:28px;text-align:center;bottom:15px;}
.carList li a.sjOrder{background:#fca43a;right:95px;}
.carList li a.djAsk{background:#83cf51;right:15px;}
.carList li span{display:block;width:100%;overflow:hidden;}
.carList li a.name{font-size:1rem;color:#717171;margin-bottom:0.5rem;position:static;width:100%;height:auto;text-align:left;border-radius:0}
.carList li span.price{color:#b2b2b2}
.carList li span.thisPrice{color:#fca43a}

.sjAdj{overflow:hidden;margin-bottom:10px;}
.sjAdj a{display:inline-block;width:48%;border-radius:5px;color:#fff;font-size:1rem;text-align:center;height:44px;line-height:44px;}
.sjAdj a.sjOrder{background:#fca43a;float:left;}
.sjAdj a.djAsk{background:#83cf51;float:right;}

.filter{background:#8f8f8f;height:30px;line-height:30px;color:#fff;font-size:0.8rem;padding:5px 15px}
.filter a{display:inline-block;padding:0 10px;border-radius:5px;}
.filter a.current{background:#828282;}

.carDetailTitle{background:#fafafa;padding:5px 10px;font-size:0.8rem;position:relative;border-top:1px solid #e3e3e3;}
.carDetailTitle .tips{position:absolute;right:10px;top:5px;color:#464646}
.carDetailTitle .tips span{margin-left:5px;}
.carDetailTitle .tips span:before, .carDetail .bp:before, .carDetail .xp:before, .carDetail .none:before{font-family:'FontAwesome';margin-right:5px;}
.carDetailTitle .bp:before, .carDetail .bp:before{content:'\f111'}
.carDetailTitle .xp:before, .carDetail .xp:before{content:'\f10c'}
.carDetailTitle .none:before, .carDetail .none:before{content:'-'}
.carDetail{border-top:1px solid #e3e3e3;width:100%;font-size:0.8rem;margin-bottom:-1px;color:#595959}
.carDetail td{width:100%;overflow:hidden;border-bottom:1px solid #e3e3e3;padding:5px 10px;}
.carDetail td.label{width:40%;text-align:right;background:#fafafa;border-right:1px solid #e3e3e3;}
.carDetail td.value{width:60%;text-align:center;}

.qiandao{padding:40px 20px;}
.qiandao .sao{text-align:center;font-size:1.2rem;border-bottom:1px dashed #e3e3e3;padding:0 20px 20px}
.qiandao .sao div{margin-bottom:1rem;}
.qiandao .sao a{background:#83cf51;color:#fff;width:100%;display:block;border-radius:5px;padding:5px 0}
.qiandao .phone{text-align:center;font-size:1.2rem;padding:20px 20px}
.qiandao .phone div{margin-bottom:1rem;overflow:hidden;}
.qiandao .phone .text{width:60%;border:1px solid #e3e3e3;float:left;display:inline-block;height:30px;border-top-left-radius:5px;border-bottom-left-radius:5px;}
.qiandao .phone .submit{width:30%;float:left;display:inline-block;background:#83cf51;color:#fff;border:0 none;height:32px;border-top-right-radius:5px;border-bottom-right-radius:5px;}
.qiandaoRen li{font-size:1rem}
.qiandaoRen .footTip{color:#9c9c9e;font-size:0.6rem;text-align:center;margin:0 auto 10px;}
.qiandaoRen .userName{text-align:center;font-size:2rem;line-height:1.8rem;margin:2rem auto}
.qiandaoRen .userName span{display:block;font-size:1rem;}

/*bl*/
/*我的顾问*/
.myagent_box{ width:90%; height:18rem; border:1px solid #CCC; margin:1rem auto 0.2rem; border-radius:0.3rem; position:relative;}
.myagent_heart{ width:2rem; height:2rem; border-radius:50%; display:block; position:absolute; top:0.5rem; right:1rem; background:url(../images/heart_icon.png) no-repeat #ccc; background-size:85% 75%;background-position:center center; padding:2px;box-sizing:border-box;}

.agent_icon{ width:6.5rem; height:6.5rem; border:1px solid #CCC; border-radius:50%; display:block; margin:1.2rem auto 0; padding:2px;}
.myagent_box p{ text-align:center; font-size:1.2rem; color:#444; margin-top:0.5rem; line-height:1.4rem;}
.myagent_box h4{ text-align:center; font-size:0.9rem; color:#999;}
.contact_box{ width:100%; height:2.8rem; background:#83cf51; position:absolute;bottom:0rem; border-bottom-left-radius:0.3rem; border-bottom-right-radius:0.3rem; padding-top:0.4rem;box-sizing:border-box; }
.contact_box a{width:49.8%; height:2rem; border-right:1px solid #65ba2c;display:block;float:left; padding-top:3px;box-sizing:border-box;}
.contact_box a:last-child{ border-right: none;}
.contact_box a h3{ width:5rem; height:26px; line-height:26px; padding-left:2rem; margin:0 auto; font-size:1rem; color:#FFF;box-sizing:border-box;}
.contact_box a h3.phone{background:url(../../img/wxIMG/phone_icon.png) no-repeat; background-size:35% 100%;}
.contact_box a h3.wechart{background:url(../../img/wxIMG/weichart_icon.png) no-repeat; background-size:35% 100%;}
.contact_result{ width:90%; height:25px;margin:0 auto; padding:1rem 1rem 0;}
.contact_result a{ display:block; font-size:0.85rem; color:#999; text-decoration: underline;}

/*表扬*/
.title_box{ width:100%; height:2.3rem; background:#4b4b4b; padding-top:8px; box-sizing:border-box; position:fixed; top:0px; left:0px; z-index:10;}
.title_box .square{ width:16px; height:18px; background:#83cf51;float:left;}
.title_box .square span{ width:0px; height:0px; border-top:9px solid transparent; border-bottom:9px solid transparent; border-left:12px solid #83cf51; position:absolute; left:1rem; display:block;}
.title_box a{font-size:1rem; color:#FFF; position:absolute; top:0.3rem; left:2rem;}
.formList li label.bordernone{border-right:none;}


/*新增顾问*/
.mar_top1{ margin-top:1rem;}
.guwen_list{ width:100%; maxheight:80%; overflow:scroll;}
.guwen_list li{ width:33.1%; height:8.2rem; border-right:1px solid #ccc;position:relative;
 border-bottom:1px solid #ccc; float:left;}
.guwen_list li:nth-child(3n){ border-right:none;}
.guwen_list li p{ font-size:0.95rem; text-align:center; color:#444;}
.guwen_select{ width:5.5rem; height:5.5rem; border:1px solid #CCC; border-radius:50%; display:block; padding:2px; margin:0.6rem auto 0; }
.guwen_list li.current:after{content:'\f103';font-family:'FontAwesome';color:#fff;background:#f00;border:3px solid #fff;width:20px;line-height:20px;height:20px;text-align:center;display:block;position:absolute;right:10%;top:10%;border-radius:50%;font-size:1rem}

/*企业信息*/
.com_logo{ width:90%; height:12rem;background:url(../images/logo.jpg) no-repeat; background-size:100% 100%; margin:0.5rem auto;}
.com_contact{ width:100%; padding-bottom:1rem; border-top:1px solid #CCC; }
.com_contact li{ width:100%; padding:0.6rem 1rem 0.6rem 0.8rem; border-bottom:1px solid #CCC; font-size:1rem; box-sizing:border-box; padding-left:12%;}

.com_contact li.address{background:url(../../img/wxIMG/dizhi.png) no-repeat; background-size:10%; background-position:0.3rem center;}
.com_contact li.phone_little{background:url(../../img/wxIMG/phone.png) no-repeat; background-size:11%; background-position:0.3rem center;}
.com_contact li.phone_little>span{ color: #666;}
.com_contact li.zhaopin{background:url(../../img/wxIMG/zhaopin.png) no-repeat; background-size:9%; background-position:0.45rem center;}

.com_info{ width:100%; padding:0.2rem 1rem 1.2rem; font-size:0.9rem; margin:0 auto; text-indent:1.6rem; border-bottom:1px solid #CCC; color:#333; box-sizing:border-box;}
.com_recommend{ width:100%; padding:0.2rem 0.2rem 2rem; font-size:0.9rem; margin:0 auto; border-bottom:1px solid #CCC; color:#333; box-sizing:border-box;}
.com_recommend h3{ text-align:left; padding-left:1rem; font-size:1rem; box-sizing:border-box;}
.com_recommend p{ width:95%; text-align:left; font-size:0.8rem; margin:0 auto;box-sizing:border-box;}
.com_recommend p:before{width:20px;height:20px; text-align:left; padding-left:1.1rem; font-size:0.85rem; box-sizing:border-box;content:'\f111';font-family:'FontAwesome';border-radius:50%; margin-right:0.1rem;}
.com_recommend .ewm{ width:10rem; height:10rem; display:block; margin:0.8rem auto;}