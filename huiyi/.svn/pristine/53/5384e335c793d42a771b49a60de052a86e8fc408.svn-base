package com.mvc.search.model;

import java.io.Serializable;
import java.util.List;

public class ProductSearchData implements Serializable{
    private static final long serialVersionUID = -7846962586043678318L;
    private List<ProductDocument> products;
    private List<FacetResult> skuAttributeFacets;
    private List<FacetResult> categoryFacets;
    private List<FacetResult> productAttributeFacets;
    public List<ProductDocument> getProducts() {
        return products;
    }
    public void setProducts(List<ProductDocument> products) {
        this.products = products;
    }
    public List<FacetResult> getSkuAttributeFacets() {
        return skuAttributeFacets;
    }
    public void setSkuAttributeFacets(List<FacetResult> skuAttributeFacets) {
        this.skuAttributeFacets = skuAttributeFacets;
    }
    public List<FacetResult> getProductAttributeFacets() {
        return productAttributeFacets;
    }
    public void setProductAttributeFacets(List<FacetResult> productAttributeFacets) {
        this.productAttributeFacets = productAttributeFacets;
    }
    public List<FacetResult> getCategoryFacets() {
        return categoryFacets;
    }
    public void setCategoryFacets(List<FacetResult> categoryFacets) {
        this.categoryFacets = categoryFacets;
    }


}
